<?xml version='1.0' encoding='utf-8'?>
<widget id="web.id.randyanwar.ecp2randy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ECP2RANDY</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="randy.anwar@gmail.com" href="http://randyanwar.web.id">
        R. Anwar Romadhonny - I'm Human, I'm Learner
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/img/ayas.jpg" />
        <icon density="mdpi" src="www/img/ayas.jpg" />
        <icon density="hdpi" src="www/img/ayas.jpg" />
        <icon density="xhdpi" src="www/img/ayas.jpg" />
        <icon density="xxhdpi" src="www/img/ayas.jpg" />
        <icon density="xxxhdpi" src="www/img/ayas.jpg" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
